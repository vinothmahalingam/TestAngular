node{

    stage('Checkout SCM'){
         git credentialsId: 'vinothmahalingam', url: 'https://github.com/vinothmahalingam/TestAngular.git', branch: 'master'
    }
    stage('Install node modules'){
        "npm install"
     }
    stage('Build'){
        "ng build --prod --output-path C:\\inetpub\\wwwroot\\TestAngular"
     }
}