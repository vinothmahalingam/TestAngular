node{

    stage('Checkout SCM'){
         git credentialsId: 'vinothmahalingam', url: 'https://github.com/vinothmahalingam/TestWebAPI.git', branch: 'master'
    }
    stage('Install node modules'){
        sh "npm install"
     }
        stage('Build'){
        sh "ng build --prod --output-path C:\\inetpub\\wwwroot\\TestAngular"
     }
}